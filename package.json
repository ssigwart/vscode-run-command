{
  "name": "run-command",
	"displayName": "Run Command",
	"description": "Run a command with the current selection as the input",
	"version": "1.0.4",
	"publisher": "ssigwart",
	"repository": {
		"type": "git",
		"url": "https://github.com/ssigwart/vscode-run-command"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#5ec900",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"terminal",
		"shell",
		"bash",
		"run command"
	],
	"activationEvents": [
		"onCommand:run-command.runCommand"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "run-command.runCommand",
				"title": "Run Command",
				"enablement": "editorTextFocus"
			}
		],
		"keybindings": [
			{
				"command": "run-command.runCommand",
				"mac": "cmd+r",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
